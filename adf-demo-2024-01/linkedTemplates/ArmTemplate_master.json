{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-demo-2024-01"
		},
		"ls_kv_demo_adf_2024_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-demo-adf-2024.vault.azure.net/"
		},
		"ls_sqldb_demo_dw_2024_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-demo-2024.database.windows.net"
		},
		"ls_sqldb_demo_dw_2024_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb-demo-dw-2024"
		},
		"ls_stackoverflow2013_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-FAHJ1AD"
		},
		"ls_stackoverflow2013_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "StackOverflow2013"
		},
		"ls_stackoverflow2013_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adf-etl-demo-2024"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-demo-2024-01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_kv_demo_adf_2024_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kv_demo_adf_2024_properties_typeProperties_baseUrl')]"
					},
					"ls_sqldb_demo_dw_2024_properties_typeProperties_server": {
						"value": "[parameters('ls_sqldb_demo_dw_2024_properties_typeProperties_server')]"
					},
					"ls_sqldb_demo_dw_2024_properties_typeProperties_database": {
						"value": "[parameters('ls_sqldb_demo_dw_2024_properties_typeProperties_database')]"
					},
					"ls_stackoverflow2013_properties_typeProperties_server": {
						"value": "[parameters('ls_stackoverflow2013_properties_typeProperties_server')]"
					},
					"ls_stackoverflow2013_properties_typeProperties_database": {
						"value": "[parameters('ls_stackoverflow2013_properties_typeProperties_database')]"
					},
					"ls_stackoverflow2013_properties_typeProperties_userName": {
						"value": "[parameters('ls_stackoverflow2013_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}